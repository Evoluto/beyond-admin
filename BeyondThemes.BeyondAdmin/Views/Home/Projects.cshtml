@model IEnumerable<BeyondThemes.BeyondAdmin.Models.ProjectViewModel>
@{
    ViewBag.Title = "Projects";
    ViewBag.Description = "projects";
    Layout = "~/Views/Shared/_Default.cshtml";
}   

<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header ">
                <span class="widget-caption">Projects</span>
                <div class="widget-buttons">
                    <a href="#" data-toggle="maximize">
                        <i class="fa fa-expand"></i>
                    </a>
                    <a href="#" data-toggle="collapse">
                        <i class="fa fa-minus"></i>
                    </a>
                    <a href="#" data-toggle="dispose">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <a id="editabledatatable_new" href="javascript:void(0);" class="btn btn-default">
                        Add New Projects
                    </a>
                    <div class="btn-group pull-right">
                        <a class="btn btn-default" href="javascript:void(0);">Tools</a>
                        <a class="btn btn-default dropdown-toggle" data-toggle="dropdown" href="javascript:void(0);"><i class="fa fa-angle-down"></i></a>
                        <ul class="dropdown-menu dropdown-default">
                            <li>
                                <a href="javascript:void(0);">Action</a>
                            </li>
                            <li>
                                <a href="javascript:void(0);">Another action</a>
                            </li>
                            <li>
                                <a href="javascript:void(0);">Something else here</a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a href="javascript:void(0);">Separated link</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <table class="table table-striped table-hover table-bordered" id="editabledatatable">
                    <thead>
                        <tr role="row">
                            <th>
                                @Html.DisplayNameFor(model => model.Tbl386_name)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Tbl386_custom_sector)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Tbl385_agency_name)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Tbl386_custom_island)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Tbl386_custom_pa_category)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Tbl386_custom_pw_number)
                            </th>
                            <th></th>
                        </tr>
                    </thead>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Tbl386_name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Tbl386_custom_sector)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Tbl385_agency_name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Tbl386_custom_island)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Tbl386_custom_pa_category)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Tbl386_custom_pw_number)
                            </td>
                            <td>
                                <a href="#" class="btn btn-info btn-xs edit"><i class="fa fa-edit"></i> Edit</a>
                                <a href="#" class="btn btn-danger btn-xs delete"><i class="fa fa-trash-o"></i> Delete</a>
                                @*@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                 @Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
                            </td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>
</div>

@section PageScripts{
    <link href="~/assets/css/dataTables.bootstrap.css" rel="stylesheet" />
    <script src="~/assets/js/datatable/jquery.dataTables.min.js"></script>
    <script src="~/assets/js/datatable/ZeroClipboard.js"></script>
    <script src="~/assets/js/datatable/dataTables.tableTools.min.js"></script>
    <script src="~/assets/js/datatable/dataTables.bootstrap.min.js"></script>
    <script src="~/assets/js/datatable/datatables-init.js"></script>
    <script>
        //InitiateEditableDataTable.init();
    </script>
}